    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 200,
      "panels": [],
      "title": "PostgreSQL Health & Performance",
      "type": "row"
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 6 },
      "id": 201,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "fieldConfig": {
        "defaults": { "unit": "bytes", "color": { "mode": "palette-classic" } }
      },
      "targets": [{
        "expr": "pg_database_size_bytes{datname='airflow'}",
        "legendFormat": "DB Size",
        "refId": "A"
      }],
      "title": "Database Size",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 6 },
      "id": 202,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [{
        "expr": "sum(pg_stat_activity_count{datname='airflow'})",
        "legendFormat": "Connections",
        "refId": "A"
      }],
      "title": "Active Connections",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 6 },
      "id": 203,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "fieldConfig": {
        "defaults": { "unit": "percent", "max": 100, "min": 0, "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 90 }] } }
      },
      "targets": [{
        "expr": "sum(pg_stat_database_blks_hit{datname='airflow'}) / (sum(pg_stat_database_blks_hit{datname='airflow'}) + sum(pg_stat_database_blks_read{datname='airflow'})) * 100",
        "legendFormat": "Hit Ratio",
        "refId": "A"
      }],
      "title": "Buffer Cache Hit Ratio",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 6 },
      "id": 204,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": { "calcs": ["mean"], "fields": "", "values": false }
      },
      "targets": [{
        "expr": "pg_up",
        "legendFormat": "Status",
        "refId": "A"
      }],
       "fieldConfig": {
        "defaults": { "mappings": [{ "type": "value", "options": { "1": { "text": "UP", "color": "green" }, "0": { "text": "DOWN", "color": "red" } } }] }
      },
      "title": "Instance Status",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 11 },
      "id": 205,
      "title": "Transactions Per Second (TPS)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit{datname='airflow'}[5m])",
          "legendFormat": "Commits",
          "refId": "A"
        },
        {
          "expr": "rate(pg_stat_database_xact_rollback{datname='airflow'}[5m])",
          "legendFormat": "Rollbacks",
          "refId": "B"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 11 },
      "id": 206,
      "title": "Tuple Activity (Rows)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(pg_stat_database_tup_fetched{datname='airflow'}[5m])",
          "legendFormat": "Fetched",
          "refId": "A"
        },
        {
          "expr": "rate(pg_stat_database_tup_inserted{datname='airflow'}[5m])",
          "legendFormat": "Inserted",
          "refId": "B"
        },
         {
          "expr": "rate(pg_stat_database_tup_updated{datname='airflow'}[5m])",
          "legendFormat": "Updated",
          "refId": "C"
        }
      ]
    }
  ],
